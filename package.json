    "css-to-style": "1.4.0",
    "document-register-element": "1.14.10",
    "eslint-plugin-mobx": "0.0.13",
    "handlebars-to-jsx": "0.1.5",
    "is-self-closing": "1.0.1",
    "lodash": "4.17.23",
    "mobx": "6.15.0",
