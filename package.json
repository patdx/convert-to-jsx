    "mobx-utils": "^6.1.0",
    "react": "19.2.4",
    "react-attr-converter": "0.3.1",
    "react-dom": "19.2.4",
    "react-is": "19.2.4",
    "regenerator-runtime": "0.14.1",
    "tslib": "2.8.1",
    "uglify-js": "^3.19.3"
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@babel/preset-env": "7.29.0",
