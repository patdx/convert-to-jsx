{
  "name": "ctj",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "wireit",
    "build": "wireit",
    "preview": "wireit",
    "lint": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "dev": {
      "command": "vite --config vite.config.ts"
    },
    "build": {
      "command": "vite build --config vite.config.ts"
    },
    "preview": {
      "command": "vite preview --config vite.config.ts",
      "dependencies": [
        "build"
      ]
    },
    "lint": {
      "command": "eslint apps/website/src"
    },
    "test": {
      "dependencies": [
        "test:angular-to-jsx"
      ]
    },
    "test:angular-to-jsx": {
      "command": "jest --config libs/angular-to-jsx/jest.config.ts",
      "files": [
        "libs/angular-to-jsx/src/**/*.ts",
        "libs/angular-to-jsx/src/**/*.tsx"
      ],
      "output": [
        "coverage/libs/angular-to-jsx"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/compiler": "21.1.2",
    "@glimmer/syntax": "0.95.0",
    "babel-plugin-transform-react-pug": "^7.0.1",
